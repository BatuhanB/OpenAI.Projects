@model Completion
<div class="container-fluid">

	<h1 class="h-100">This is a Completion Page</h1>

	<div class="row">
		<div class="col4">
			1 of 2
		</div>
		<div class="col8">
			<div class="row">
				<form asp-action="GetAsync" method="get" asp-controller="Completion">
					<div class="form-group">
						<label asp-for="Prompt">Enter the Prompt</label>
						<input asp-for="Prompt"  type="text" class="form-control" />
						<span asp-validation-for="Prompt" class="text-danger"></span>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>